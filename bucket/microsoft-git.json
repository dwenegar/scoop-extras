{
    "version": "2.34.1.vfs.0.0",
    "description": "Distributed version control system",
    "homepage": "https://github.com/microsoft/git",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://github.com/microsoft/git/releases/download/v2.34.1.vfs.0.0/PortableGit-2.34.1.vfs.0.0-64-bit.7z.exe#/dl.7z",
            "hash": "2533adaaa337b0ac3c77b1bc4e5aada4f681e6bfa74e9fdfb3705389f4eaec08"
        }
    },
    "env_add_path": "cmd",
    "bin": [
        "usr\\bin\\tig.exe",
        "git-bash.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "checkver": {
        "github": "https://github.com/microsoft/git",
        "regex": "v([\\w.]+)/PortableGit-(?<full>[\\w.]+)-64-bit"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/microsoft/git/releases/download/v$version/PortableGit-$matchFull-64-bit.7z.exe#/dl.7z"
            }
        },
        "hash": {
            "url": "https://github.com/microsoft/git/releases/tag/v$version",
            "regex": "<td>$basename</td>\\s*<td>$sha256</td>"
        }
    }
}
